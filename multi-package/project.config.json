{
  "mode": "multi",
  "lint": {
    "autoFix": true,
    "root": "src",
    "ext": [".js",".jsx"],
    "ignore": ["assets"]
  },
  "pub": {
    "assetsRir": "dist",
    "assetsPath": "",
    "sourceMap": false,
    "devtool": "#source-map",
    "gzip": true,
    "gzipExtensions": ["js", "css"],
    "analyzerReport": false,
    "loaders": [],
    "plugins": []
  },
  "dev": {
    "subassetsRir": "static",
    "assetsPath": "/",
    "proxyTable": {},
    "host": "0.0.0.0",
    "port": 8088,
    "autoOpenBrowser": false,
    "errorOverlay": true,
    "notifyOnErrors": true,
    "poll": false,
    "useEslint": true,
    "useStylelint": true,
    "showEslintErrorsInOverlay": false,
    "devtool": "cheap-module-eval-source-map",
    "cacheBusting": true,
    "cssSourceMap": true,
    "loaders": [],
    "plugins": []
  }
}
